<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="700.0" prefWidth="800.0" style="-fx-background-color: #bef1ff;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.pokemon.AvisoController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Button fx:id="botonMenu" layoutX="415.0" layoutY="205.0" mnemonicParsing="false" onAction="#menu" prefHeight="199.0" prefWidth="334.0" text="VOLVER AL MENÚ">
         <font>
            <Font name="Comic Sans MS" size="29.0" />
         </font></Button>
      <Button fx:id="salir" layoutX="57.0" layoutY="205.0" mnemonicParsing="false" onAction="#salir" prefHeight="199.0" prefWidth="334.0" text="SALIR">
         <font>
            <Font name="Comic Sans MS" size="29.0" />
         </font>
      </Button>
      <Label layoutX="59.0" layoutY="125.0" text="¿SEGURO QUE QUIERES SALIR? SI SALES SE PERDERÁN LOS DATOS GUARDADOS">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextArea fx:id="lblLogs" editable="false" layoutX="57.0" layoutY="570.0" prefHeight="102.0" prefWidth="683.0" wrapText="true" />
   </children>
</Pane>
